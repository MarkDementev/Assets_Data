<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
            http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="create-test-asset-relationship-table" author="MarkDementev">
        <createTable tableName="abstract_asset_ownerships_with_account_placement">
            <column name="id" type="BIGINT">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="asset_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
<!--            <column name="surname" type="varchar">-->
<!--                <constraints nullable="false"/>-->
<!--            </column>-->
            <column name="created_at" type="TIMESTAMP(3) WITH TIMEZONE">
                <constraints nullable="false"/>
            </column>
            <column name="updated_at" type="TIMESTAMP(3) WITH TIMEZONE">
                <constraints nullable="false"/>
            </column>
        </createTable>

        <addForeignKeyConstraint constraintName="fkha3t5j0pyhdrdxleiuwh49jk9"
                                 baseTableName="abstract_asset_ownerships_with_account_placement"
                                 baseColumnNames="asset_id"
                                 referencedTableName="abstract_assets"
                                 referencedColumnNames="id"/>
    </changeSet>

</databaseChangeLog>